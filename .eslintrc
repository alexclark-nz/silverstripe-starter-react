{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1,
    "react/jsx-props-no-spreading": 0,
    "global-require": 0,
    "import/no-extraneous-dependencies": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80,
        "tabWidth": 4
      }
    ]
  }
}
